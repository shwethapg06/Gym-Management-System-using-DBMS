    private void updateActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_updateActionPerformed
        // TODO add your handling code here:
        String SQL="UPDATE Trainer SET Full_name='"+fullnametf.getText()+"', Email_id='"+emailidtf.getText()+"', Contact_no='"+contactnotf.getText()+"', Address='"+addresstf.getText()+"' where Trainer_id='"+traineridtf.getText()+"'";
               int retVal=DBOperations.UPDATE_Table(SQL);
                if(retVal==1){
                   JOptionPane.showMessageDialog(this, "Operation succesfully completed!!", "Member record insertion..", 1);
                   this.dispose();
                }
                else{
                    JOptionPane.showMessageDialog(this, "Operation failed!!", "Member record insertion..", 1);
                    this.dispose();
                }
    }//GEN-LAST:event_updateActionPerformed

