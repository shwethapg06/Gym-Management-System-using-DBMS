    private void savebtnActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_savebtnActionPerformed
        // TODO add your handling code here:
         if (savebtn.getText().equals("Add")){
            fullnametf.setText("");
            memberidtf.setText("");
            batchidtf.setText("");
            //gendertf.setText("");
            agetf.setText("");
            contactnotf.setText("");
            emailidtf.setText("");
            addresstf.setText("");
            weighttf.setText("");
            heighttf.setText("");
            
            fullnametf.setEditable(true);
            memberidtf.setEditable(true);
            batchidtf.setEditable(true);
           // gendertf.setEditable(true);
            agetf.setEditable(true);
            contactnotf.setEditable(true);
            emailidtf.setEditable(true);
            addresstf.setEditable(true);
            weighttf.setEditable(true);
            heighttf.setEditable(true);

            savebtn.setText("Save");
        }
        else{
            if( !fullnametf.getText().equals("") && !memberidtf.getText().equals("") && !batchidtf.getText().equals("") && !gendertf.equals("") && !agetf.getText().equals("") && !contactnotf.getText().equals("") && !emailidtf.getText().equals("") &&  !addresstf.getText().equals("") && !weighttf.getText().equals("") && !heighttf.getText().equals("")){
                String SQL="INSERT INTO Member VALUES ('"+fullnametf.getText().trim()+"','" +memberidtf.getText().trim()+"','"+batchidtf.getText().trim()+"','"+gendertf.trim()+"','"+agetf.getText().trim()+"','"+contactnotf.getText().trim()+"','"+emailidtf.getText().trim()+"','"+addresstf.getText().trim()+"','"+weighttf.getText().trim()+"','"+heighttf.getText().trim()+"')";
                int retVal=DBOperations.UPDATE_Table(SQL);
                if(retVal==-1){
                   JOptionPane.showMessageDialog(this, "Operation Failed", "Member record insertion..", 1);
                   this.dispose();
                }
                else{
                    JOptionPane.showMessageDialog(this, "Operation successfully completed", "Member record insertion..", 1);
                    this.dispose();
                }
            fullnametf.setText("");
            memberidtf.setText("");
            batchidtf.setText("");
            //gendertf.setText("");
            agetf.setText("");
            contactnotf.setText("");
            emailidtf.setText("");
            addresstf.setText("");
            weighttf.setText("");
            heighttf.setText("");
            
                
                savebtn.setText("Add");
                
                fullnametf.setEditable(false);
            memberidtf.setEditable(false);
            batchidtf.setEditable(false);
           // gendertf.setEditable(false);
            agetf.setEditable(false);
            contactnotf.setEditable(false);
            emailidtf.setEditable(false);
            addresstf.setEditable(false);
            weighttf.setEditable(false);
            heighttf.setEditable(false);
               

        }  
        }   
        
                                         
       
    }//GEN-LAST:event_savebtnActionPerformed

